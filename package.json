{
  "name": "django-commands",
  "displayName": "Django Commands",
  "description": "Extension to easily run and debug django commands",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "django-commands.run",
        "title": "Django Commands: Run"
      },
      {
        "command": "django-commands.debug",
        "title": "Django Commands: Debug"
      },
      {
        "command": "django-commands.reload",
        "title": "Django Commands: Reload"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.102.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "eslint": "^9.25.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}